+&pinctrl_periphs {
+	/* audio pin mux */
+
+	tdma_mclk: tdma_mclk {
+		mux { /* GPIOH_4 */
+			groups = "mclk0_h";
+			function = "mclk0";
+			drive-strength = <1>;
+		};
+	};
+
+	tdmout_a: tdmout_a {
+		mux { /* GPIOH_5, GPIOH_6, GPIOH_7, GPIOH_8*/
+			groups = "tdma_fs_h",
+				"tdma_sclk_h",
+				"tdma_dout0_h",
+				"tdma_dout1_h";
+			function = "tdma_out";
+			drive-strength = <1>;
+			bias-pull-down;
+		};
+	};
+
+	tdmout_c: tdmout_c {
+		mux { /* GPIODV_7, GPIODV_8, GPIODV_9 */
+			groups = "tdmc_sclk",
+				"tdmc_fs",
+				"tdmc_dout0";
+			function = "tdmc_out";
+			drive-strength = <1>;
+		};
+	};
+
+	tdmin_c: tdmin_c {
+		mux { /* GPIODV_10 */
+			groups = "tdmc_din1";
+			function = "tdmc_in";
+			drive-strength = <1>;
+		};
+	};
+
+	spdifin_a: spdifin_a {
+		mux { /* GPIODV_5 */
+			groups = "spdif_in";
+			function = "spdif_in";
+		};
+	};
+
+	spdifout_a: spdifout_a {
+		mux { /* GPIODV_4 */
+			groups = "spdif_out_dv4";
+			function = "spdif_out";
+		};
+	};
+
