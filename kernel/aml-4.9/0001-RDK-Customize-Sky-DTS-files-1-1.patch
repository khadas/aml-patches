+			reusable;
+			size = <0x01000000>;
+			alignment = <0x00400000>;
+			alloc-ranges = <0x30000000 0x01000000>;
+		};
+	}; /* end of reserved-memory */
+	galcore {
+		status = "okay";
+	};
+	codec_mm {
+		compatible = "amlogic, codec, mm";
+		status = "okay";
+		memory-region = <&codec_mm_cma &codec_mm_reserved>;
+	};
+
